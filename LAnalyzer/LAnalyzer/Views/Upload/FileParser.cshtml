@model LAnalyzer.Models.CSVFile
@{
    ViewBag.Title = "FileParser";
}

<h2>FileParser</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-actions no-color">
    <input type="submit" value="Save" class="btn btn-default" /> |
    @Html.ActionLink("Save_File_Data", new {CSV })
</div>
}

<table>
    <tr style="font-weight:bold">
        <td>Column Name</td>
        <td>Data Type</td>
        @for (int i = 1; i < 11; i++)
        {
            <td>Row @i</td>
        }

    </tr>

    @{
        int j = 0;
        foreach (var termName in Model.NameList)
        {

            <tr>
                <td>@termName</td>
                <td>@Model.TypeList[j]</td>
                @for (int i = 0; i < 10; i++)
                {
                    var x = Model.ValueList.ToList()[i];
                    <td>@(((List<string>)x)[j])</td>
                }

            </tr>
            j++;
        }
    }

</table>

